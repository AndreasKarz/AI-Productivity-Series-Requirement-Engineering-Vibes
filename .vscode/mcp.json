{
  "inputs": [
    {
      "id": "ado_org",
      "type": "promptString",
      "description": "Azure DevOps organization name  (e.g. 'contoso')"
    }
  ],
  "servers": {
    "ado": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@azure-devops/mcp@latest",
        "${input:ado_org}"
      ]
    },
    "sequential-thinking": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ]
    },
    "playwright": {
      "command": "npx",
      "args": [
        "@playwright/mcp@latest"
      ],
      "type": "stdio"
    },
    "microsoft-docs": {
      "type": "http",
      "url": "https://learn.microsoft.com/api/mcp"
    },
    "memory": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory@latest"
      ],
      "env": {
        "MEMORY_FILE_PATH": "${env:USERPROFILE}\\.vscode-insiders\\mcp-memory.json"
      }
    },
    "MongoDB": {
      "command": "npx",
      "args": [
        "-y",
        "mongodb-mcp-server@latest",
        "--readOnly"
      ],
      "env": {
        "MDB_MCP_CONNECTION_STRING": "mongodb+srv://dev:FtI9rZub4RX7c8tNjQvpH8SPUFFwqOr1@cluster-01-c3edb.azure.mongodb.net/?authSource=admin"
      }
    },
    "pdf2img": {
      "type": "stdio",
      "command": "node",
      "args": [
        ".mcp\\pdf2img\\index.js"
      ],
      "cwd": "${workspaceFolder}"
    },
    "asciidoc": {
      "type": "stdio",
      "command": "node",
      "args": [
        ".mcp\\asciidoc\\index.js"
      ],
      "cwd": "${workspaceFolder}"
    }
  }
}